RewriteEngine On

SetEnv	P3_Env	tim

# SEO Shiz
RewriteCond %{REQUEST_URI} ^/index.html
RewriteRule ^.*$ / [R=301,NC,QSA,L]


# P3 Dispatch
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]
RewriteRule ^.*$ index.php [NC,L,QSA]
